{
  "version": 2,
  "builds": [
    {
      "src": "frontendreact/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backendexpress/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontendreact/build/$1"
    }
  ]
}
